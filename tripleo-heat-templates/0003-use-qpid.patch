diff --git a/block-storage-nfs.yaml b/block-storage-nfs.yaml
index eabe983..9fba1eb 100644
--- a/block-storage-nfs.yaml
+++ b/block-storage-nfs.yaml
@@ -62,7 +62,7 @@ Resources:
         nfs_shares:
           - {"Fn::Join": ['', [{"Fn::Select": [ 0, {"Fn::Select": [ "ctlplane", {"Fn::GetAtt": [notCompute0, networks]} ]} ] } , ':/mnt/state/var/lib/nfs_share']]}
       admin-password: {Ref: AdminPassword}
-      rabbit:
+      qpid:
         host: {"Fn::Select": [ 0, {"Fn::Select": [ "ctlplane", {"Fn::GetAtt": [notCompute0, networks]} ]} ] }
         username: {Ref: RabbitUserName}
         password: {Ref: RabbitPassword}
diff --git a/block-storage.yaml b/block-storage.yaml
index bc14dc8..a4c76e9 100644
--- a/block-storage.yaml
+++ b/block-storage.yaml
@@ -79,7 +79,7 @@ Resources:
           Ref: CinderPassword
         volume: 'true'
       admin-password: {Ref: AdminPassword}
-      rabbit:
+      qpid:
         host: {"Fn::Select": [ 0, {"Fn::Select": [ "ctlplane", {"Fn::GetAtt": [notCompute0, networks]} ]} ] }
         username: {Ref: RabbitUserName}
         password: {Ref: RabbitPassword}
diff --git a/notcompute.yaml b/notcompute.yaml
index 5a4d9d2..f8be880 100644
--- a/notcompute.yaml
+++ b/notcompute.yaml
@@ -197,7 +197,7 @@ Resources:
             - - 'mysql://neutron:'
               - {Ref: NeutronDBPassword}
               - '@127.0.0.1/ovs_neutron'
-      rabbit:
+      qpid:
         host:
           '127.0.0.1'
         username:
diff --git a/nova-compute-instance.yaml b/nova-compute-instance.yaml
index d2ca90f..d297942 100644
--- a/nova-compute-instance.yaml
+++ b/nova-compute-instance.yaml
@@ -189,7 +189,7 @@ Resources:
         service-password:
           Ref: NeutronPassword
       admin-password: {Ref: AdminPassword}
-      rabbit:
+      qpid:
         host: {Ref: RabbitHost}
         username: {Ref: RabbitUserName}
         password: {Ref: RabbitPassword}
diff --git a/overcloud-source.yaml b/overcloud-source.yaml
index 90543bb..500d3c3 100644
--- a/overcloud-source.yaml
+++ b/overcloud-source.yaml
@@ -454,7 +454,7 @@ Resources:
             - SecretAccessKey
           stack_name:
             Ref: AWS::StackName
-      rabbit:
+      qpid:
         host:
           Fn::Select:
             - 0
diff --git a/undercloud-source.yaml b/undercloud-source.yaml
index f18612c..4d1461e 100644
--- a/undercloud-source.yaml
+++ b/undercloud-source.yaml
@@ -213,7 +213,7 @@ Resources:
           enable_tunneling: 'False'
         service-password:
           Ref: NeutronPassword
-      rabbit:
+      qpid:
         host: 127.0.0.1
         username:
           Ref: RabbitUserName
